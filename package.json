{
  "name": "game-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "dev:snake": "node server/snake-server.js",
    "dev:box-jump": "node server/box-jump-server.js",
    "dev:multiplayer": "node server/multiplayer-server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:multiplayer\"",
    "build": "SKIP_TYPE_CHECK=true next build",
    "start": "next start",
    "start:snake": "node server/snake-server.js",
    "start:box-jump": "node server/box-jump-server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:splash": "node scripts/generate-splash-screens.js",
    "generate:pwa-assets": "npm run generate:icons && npm run generate:splash"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.16.7",
    "@colyseus/redis-driver": "^0.15.0",
    "@colyseus/redis-presence": "^0.15.0",
    "@headlessui/react": "^2.0.0",
    "@heroicons/react": "^2.1.0",
    "@prisma/client": "^5.19.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.1.0",
    "colyseus": "^0.15.0",
    "colyseus.js": "^0.15.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "ioredis": "^5.3.2",
    "next": "^14.2.0",
    "next-auth": "^4.24.0",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "socket.io": "^4.7.2",
    "tailwind-merge": "^2.5.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.0",
    "@types/jest": "^29.5.0",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "bcryptjs": "^2.4.0",
    "concurrently": "^8.2.2",
    "critters": "^0.0.23",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prisma": "^5.19.0",
    "sharp": "^0.33.0",
    "typescript": "^5.6.0"
